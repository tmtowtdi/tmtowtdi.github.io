   [% header %]

    <!-- Intro Section
    ================================================== -->
    <section id="intro">

        <!-- Flexslider Start-->
        <div id="intro-slider" class="flexslider">
            <ul class="slides">

                [% FOR slide = slides %]
                    <li>
                        <div class="row">
                            <div class="twelve columns">
                                <div class="slider-text">
                                    <h1>[% slide.header %]<span>.</span></h1>
                                    <p>[% slide.desc %]</p>
                                </div>
                                <div class="slider-image">
                                    <img src="[% slide.image %]" alt="[% slide.image_alt %]" />
                                </div>
                            </div>
                        </div>
                    </li>
                [% END %]

            </ul>
        </div> <!-- Flexslider End-->

    </section> <!-- Intro Section End-->

   <!-- Info Section
   ================================================== -->
    <section id="info">
        <div class="row">
            <div class="bgrid-quarters s-bgrid-halves">

                [% FOR blurb = blurbs %]
                    <div class="columns">
                        <h2>[% blurb.header %]</h2>
                        <p>[% blurb.text %]</p>
                    </div>
